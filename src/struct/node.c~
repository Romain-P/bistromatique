#include "node.h"

t_node		create_node(t_node *parent)
{
  t_node	*node;

  node = malloc(sizeof(t_node));
  node->operator = 0;
  node->number = 0;
  node->left = 0;
  node->right = 0;
  node->parrent = parent;
  return (node);
}
